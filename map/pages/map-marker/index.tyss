.container {
  padding: 0 20px 20px;
}
.point {
  width: 30px;
  height: 30px;

  left: 50%;
  top: 50%;
  margin-left: -15px;
  margin-top: -15px;
  position: absolute;
  z-index: 99;
  background-image: url('/assets/location.svg');
}
.myMap {
  height: 60vh;
  width: 100%;
  position: relative;
}

.tips {
  padding: 8px 16px;
  color: red;
}
button {
  margin: 16px 0;
}

switch {
  margin: 16px 0;
}
